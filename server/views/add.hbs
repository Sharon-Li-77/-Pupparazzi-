<form class="form" action="/puppies/{{length}}/add" method="post" enctype="application/x-www-form-urlencoded">


  <img class="img-circle" src="/images/puppyProfile.jpg" alt="new-picture">

  <label for="image" class="form-item">
    Picture:
    <input type="file" name="image" >
    </label> 
  <label for="name" class="form-item">
    Name:
    <input type="text" name="name" value="">
  </label>
  
  <label for="breed" class="form-item">
    Breed:
    <input type="text" name="breed" value="">
  </label>
  
  <label for="owner" class="form-item">
    Owner:
    <input type="text" name="owner" value="">
  </label>
  
  <input type="submit" name="" value="Add">
</form>
